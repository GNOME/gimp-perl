GtkWidget *					T_IV

GimpPixelRgnIterator				T_PTROBJ

IMAGE						T_PREF
LAYER						T_PREF
COLOR						T_PREF
CHANNEL						T_PREF
DRAWABLE					T_PREF_ANY
DISPLAY						T_PREF
REGION						T_PREF

INPUT

T_PREF
	$var = unbless_croak ($arg, PKG_$ntype)

T_PREF_ANY
	$var = unbless_croak ($arg, PKG_ANY)

T_GDRAWABLE
	$var = old_gdrawable ($arg)

T_TILE
	$var = old_tile ($arg)

T_PIXELRGN
	$var = old_pixelrgn ($arg)

T_PIXELRGN_PDL
	$var = old_pixelrgn_pdl ($arg)

OUTPUT

T_PREF
	$arg = autobless (newSViv($var), GIMP_PDB_$ntype);

T_PREF_ANY
	$arg = autobless (newSViv($var), GIMP_PDB_$ntype);


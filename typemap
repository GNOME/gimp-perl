gchar						T_IV
gint						T_IV
gint32						T_IV
guchar						T_U_CHAR
guint						T_IV
gulong						T_U_LONG
guint32						T_IV
gdouble						T_DOUBLE
gchar *						T_PV
gint *						T_PTROBJ
guchar *					T_PV

GimpDrawable *					T_GDRAWABLE
GimpTile *					T_TILE

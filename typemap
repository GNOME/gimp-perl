GtkWidget *					T_IV

IMAGE						T_PREF
LAYER						T_PREF
COLOR						T_PREF
CHANNEL						T_PREF
DRAWABLE					T_PREF_ANY
DISPLAY						T_PREF
REGION						T_PREF

INPUT

T_PREF
	$var = unbless ($arg, PKG_$ntype, 0)

T_PREF_ANY
	$var = unbless ($arg, PKG_ANY, 0)

T_GDRAWABLE
	$var = old_gdrawable ($arg)

T_TILE
	$var = old_tile ($arg)

T_PIXELRGN
	$var = old_pixelrgn ($arg)

OUTPUT

T_PREF
	$arg = autobless (newSViv($var), PARAM_$ntype);

